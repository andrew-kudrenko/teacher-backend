{
  "name": "backend",
  "engines": {
    "yarn": "1.x",
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "engineStrict": true,
  "version": "1.0.0",
  "main": "dist/app.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "start:dev": "nodemon",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.22",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/mongoose": "^5.10.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.13"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
